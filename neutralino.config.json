{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/vite-src/dist/",
  "url": "/",
  "port": 3000,
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory",
    "os.showOpenDialog",
    "filesystem.readBinaryFile",
    "filesystem.writeBinaryFile",
    "os.getPath()",
    "clipboard.*",
    "filesystem.copy",
    "filesystem.writeFile",
    "filesystem.readFile",
    "filesystem.createDirectory"
  ],
  "singlePageServe": true,
  "modes": {
    "window": {
      "title": "Labs",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/vite-src/public/neutralino.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "Labs",
    "resourcesPath": "/vite-src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.2.0",
    "clientVersion": "6.2.0",
    "frontendLibrary": {
      "patchFile": "/vite-src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/vite-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}